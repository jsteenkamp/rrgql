{
  "root": true,
  "env": {
    "browser": true,
    "amd": true
  },
  "extends" : "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 5
  },
  "globals": {
    "aui": false,
    "Plotly": false,
    "moment": false,
    "chroma": false
  },
  "rules": {
    "no-console": [
      "error",
      { "allow": [
          "info",
          "error"
        ]
      }
    ],
    "no-unused-vars": 0,
    "comma-dangle": 0,
    "space-before-function-paren": [2, "never"]
  }
}